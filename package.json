{
  "name": "ts-express-decorators",
  "version": "5.0.4",
  "description": "A TypeScript Framework on top of Express",
  "main": "./dist/lib/index.js",
  "typings": "./dist/lib/index.d.ts",
  "scripts": {
    "precommit": "lint-staged",
    "postcommit": "git update-index --again",
    "postinstall": "gulp repo:bootstrap",
    "semantic-release": "semantic-release",
    "semantic-release:dry-run": "semantic-release --dry-run",
    "clean": "gulp clean:workspace",
    "start": "ts-node -r tsconfig-paths/register -r source-map-support/register test/integration/app/app.ts",
    "test": "npm run test:lint && npm run test:unit",
    "test:unit": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text _mocha --recursive",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "travis:deploy-once": "travis-deploy-once",
    "travis:coveralls": "nyc report --reporter=text-lcov -r ts-node/register -r tsconfig-paths/register -r source-map-support/register \"**/*.spec.ts\" | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "build": "gulp repo:build",
    "repo:build": "gulp repo:build",
    "repo:bootstrap": "gulp repo:bootstrap",
    "docs:serve": "gulp docs:serve",
    "docs:build:api": "gulp docs:buildApi",
    "docs:build": "gulp docs:build",
    "prettier": "prettier '{src,test}/**/*.ts' --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Romakita/ts-express-decorators.git"
  },
  "keywords": [
    "Express",
    "TypeScript",
    "decorators",
    "router",
    "controllers",
    "services",
    "middlewares",
    "models",
    "mvc",
    "injection",
    "ioc",
    "di",
    "api",
    "swagger",
    "mongoose",
    "ajv",
    "socketio",
    "ES2015",
    "ES6",
    "server",
    "rest",
    "validation",
    "multer"
  ],
  "author": "Romain Lenzotti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Romakita/ts-express-decorators/issues"
  },
  "homepage": "http://tsed.io/",
  "dependencies": {
    "@types/json-schema": "^7.0.0",
    "globby": "^8.0.1",
    "reflect-metadata": "^0.1.12",
    "ts-httpexceptions": "^3.0.2",
    "ts-log-debug": "^4.0.1"
  },
  "devDependencies": {
    "@typedproject/ts-doc": "^4.0.1",
    "@types/chai": "^4.0.1",
    "@types/chai-as-promised": "7.1.0",
    "@types/cookie-parser": "^1.3.29",
    "@types/express": "^4.0.32",
    "@types/express-session": "^1.15.11",
    "@types/globby": "^8.0.0",
    "@types/mocha": "^5.2.0",
    "@types/mongoose": "^5.0.6",
    "@types/multer": "^1.3.6",
    "@types/node": "^10.9.2",
    "@types/proxyquire": "^1.3.27",
    "@types/sinon": "^5.0.2",
    "@types/sinon-chai": "^3.2.0",
    "@types/socket.io": "^1.4.31",
    "@types/superagent": "^3.5.3",
    "@types/supertest": "^2.0.0",
    "@types/swagger-schema-official": "^2.0.9",
    "ajv": "^6.5.0",
    "body-parser": "^1.15.1",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "chalk": "^2.4.1",
    "compression": "^1.6.1",
    "concurrently": "^4.0.1",
    "cookie-parser": "^1.4.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "ejs": "^2.5.7",
    "ejs-lint": "^0.3.0",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "fancy-log": "^1.3.2",
    "gflow": "3.0.8",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-exec": "^3.0.1",
    "gulp-json-editor": "^2.2.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-replace": "^1.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^4.0.1",
    "html-minifier": "^3.5.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "method-override": "^3.0.0",
    "mocha": "^5.1.1",
    "mongoose": "^5.0.9",
    "multer": "^1.3.0",
    "nyc": "^13.0.1",
    "prettier": "^1.12.1",
    "proxyquire": "^2.0.1",
    "read-package-json": "^2.0.12",
    "require-dir": "^1.0.0",
    "semantic-release": "^15.9.12",
    "serve-static": "^1.13.2",
    "sinon": "^6.1.5",
    "sinon-chai": "^3.0.0",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.1.0",
    "supertest": "^3.0.0",
    "swagger-ui-dist": "^3.13.6",
    "travis-deploy-once": "^5.0.4",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.8.0",
    "typeorm": "^0.2.7",
    "typescript": "^3.0.1",
    "vue-analytics": "^5.14.0",
    "vuepress": "^0.14.2",
    "vuepress-theme-tsed": "^1.3.3"
  },
  "directories": {
    "lib": "lib",
    "packages": "packages",
    "test": "test"
  }
}
